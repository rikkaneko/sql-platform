{
  "name": "sql-platform-api-server",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/api.js",
  "scripts": {
    "build": "tsc -p .",
    "start": "tsc -p .; node dist/api.js",
    "dev": "nodemon --watch 'src/**' --ext 'ts,js,json,mjs' --exec 'npm start'",
    "format": "prettier --write .",
    "lint": "eslint . --color --cache -f friendly --max-warnings 10"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.9.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.4.5",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-formatter-friendly": "^7.0.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.0.0",
    "typescript": "^5.1.6"
  }
}
